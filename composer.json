{
  "name": "davidhirtz/yii2-media",
  "type": "yii2-extension",
  "description": "Yii2 Media Management Module",
  "homepage": "https://www.davidhirtz.com/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "davidhirtz/yii2-skeleton": "@dev",
    "npm-asset/cropperjs": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "davidhirtz\\yii2\\media\\": "src"
    }
  },
  "extra": {
    "bootstrap": "davidhirtz\\yii2\\media\\composer\\Bootstrap"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require-dev": {
    "rector/rector": "^0.18.6"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
